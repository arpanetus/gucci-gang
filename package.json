{
  "name": "gucci-gang",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean transpile",
    "server": "node ./dist/index.js",
    "dev": "NODE_ENV=development NODE_PATH=. npm-run-all build server",
    "watch": "nodemon",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rm -rf dist-server",
    "populate": "./node_modules/.bin/babel-node src/entities/populate.js run",
    "populog": "./node_modules/.bin/babel-node src/entities/populate.js log"
  },
  "dependencies": {
    "@babel/node": "^7.8.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-graphql": "^0.4.1",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.11.0",
    "apollo-server-express": "^2.12.0",
    "babel-node": "^0.0.1-security",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "fs": "0.0.1-security",
    "graphql-tag": "latest",
    "morgan": "~1.9.1",
    "neo4j-driver": "^4.0.2",
    "neo4j-graphql-js": "^2.13.0",
    "node-fetch": "latest",
    "npm-run-all": "^4.1.5",
    "uuid": "^8.0.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.8.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*"
    ]
  }
}
